<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/mistyrose"

    >


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.2" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="375dp"
        android:layout_height="212dp"
        android:contentDescription="TODO"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.256"
        app:srcCompat="@drawable/func1" />

    <ImageButton
        android:id="@+id/weatherbtn"
        android:layout_width="156dp"
        android:layout_height="148dp"
        android:background="@drawable/weatherfunc"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/healthbtn"
        app:layout_constraintTop_toBottomOf="@+id/healthbtn"
        app:layout_constraintVertical_bias="0.207"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/healthbtn"
        android:layout_width="152dp"
        android:layout_height="153dp"
        android:layout_marginStart="28dp"
        android:background="@drawable/hefunc"
        android:contentDescription="TODO"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintHorizontal_bias="0.423"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.57"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/daliybtn"
        android:layout_width="149dp"
        android:layout_height="148dp"
        android:layout_marginStart="32dp"
        android:background="@drawable/daliyfun"

        app:layout_constraintBottom_toBottomOf="@+id/healthbtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/healthbtn"
        app:layout_constraintTop_toTopOf="@+id/healthbtn"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/newbtn"
        android:layout_width="156dp"
        android:layout_height="147dp"
        android:background="@drawable/newsfunc"
        app:layout_constraintBottom_toBottomOf="@+id/weatherbtn"
        app:layout_constraintEnd_toEndOf="@+id/daliybtn"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/weatherbtn"
        app:layout_constraintTop_toBottomOf="@+id/daliybtn"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="353dp"
        android:layout_height="63dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/guideline3"
        app:layout_constraintVertical_bias="0.571"
        app:srcCompat="@drawable/func2" />

    <ImageButton
        android:id="@+id/helpbtn"
        android:layout_width="153dp"
        android:layout_height="67dp"
        android:layout_marginStart="28dp"
        android:background="@drawable/helpimg"
        app:layout_constraintBottom_toTopOf="@+id/guideline3"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintHorizontal_bias="0.04"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.506"
        tools:ignore="SpeakableTextPresentCheck"/>

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="101dp"
        android:layout_height="60dp"
        android:layout_marginTop="44dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toEndOf="@+id/helpbtn"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.071"
        app:srcCompat="@drawable/voicetext" />

    <ImageButton
        android:id="@+id/voice1"
        android:layout_width="81dp"
        android:layout_height="75dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/voice"
        app:layout_constraintBottom_toTopOf="@+id/guideline3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.789"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.205"
        tools:ignore="SpeakableTextPresentCheck"/>

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="126dp"
        android:layout_height="82dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/guideline3"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/next" />

</androidx.constraintlayout.widget.ConstraintLayout>